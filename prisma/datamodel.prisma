type Book {
  id: ID! @id
  title: String!
  slug: String!
  isbn: String
  size: String
  price: Int!
  shopeLink: String
  cover: String
  photos: [String!]! @scalarList(strategy: RELATION)
  author: [Author!]!@realtion(name:"BookToAuthors",onDelete: SET_NULL)
  couress: [Couress!]!@realtion(name:"BookToCouress",onDelete: CASCADE)
  catogory: [Catogory!]!@realtion(name:"BookToCatogory",onDelete: SET_NULL)
  publiserYear: [PubliserYear!]! @realtion(name:"BookToPubliserYear",onDelete: CASCADE)
  publisher: [Publisher!]!@realtion(name:"BookToPublisher",onDelete: SET_NULL)
  classRoom: [ClassRoom!]!@realtion(name:"BookToClassRoom",onDelete: SET_NULL)
  isListed: Boolean!@default(value:false)
  description: String
  status: Int!
  subjects: [Subject!]!@realtion(name:"BookToSubject",onDelete: CASCADE)
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}
type Publisher {
  id: ID! @id
  title: String!
  slug: String!
  description: String
  phoneNumber: String
  address: String
  books: [Book!]! @realtion(name:"PublisherToBooks",onDelete: CASCADE)
  order: Int!
  isListed: Boolean!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type ClassRoom {
  id: ID! @id
  title: String!
  slug: String!
  description: String
  isListed: Boolean!
  status: Int!
  books: [Book!]! @realtion(name:"ClassRoomToBooks",onDelete: CASCADE)
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type PubliserYear {
  id: ID! @id
  title: String!
  slug: String!
  description: String
  isListed: Boolean!
  status: Int!
  books: [Book!]! @realtion(name:"PubliserYearToBooks",onDelete: CASCADE)
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type Couress {
  id: ID! @id
  title: String!
  description: String
  books: [Book!]! @realtion(name:"CouressToBooks",onDelete: CASCADE)
  isListed: Boolean!
  status: Int!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}
type Catogory {
  id: ID! @id
  title: String!
  description: String
  books: [Book!]! @realtion(name:"CatogoryToBooks",onDelete: CASCADE)
  isListed: Boolean!
  status: Int!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type Author {
  id: ID! @id
  title: String!
  description: String
  books: [Book!]! @realtion(name:"AuthorToBooks",onDelete: CASCADE)
  isListed: Boolean!
  status: Int!
  slug: String!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}


type Subject {
  id: ID! @id
  title: String!
  description: String
  books:[Book!]! @realtion(name:"SubjectToBooks",onDelete: SET_NULL)
  isListed: Boolean!
  status: Int!
  videos: [Vedio!]! @realtion(name:"SubjectToVedio",onDelete: CASCADE)
 createdAt: DateTime! @createdAt
 updatedAt: DateTime! @updatedAt
}
type BookTest {
  id: ID! @id
  title: String!
  questions: [Questions!]! @realtion(name:"BookTestToQuestions",onDelete: CASCADE)
  isListed: Boolean!
  status: Int!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}
type Vedio {
  id: ID! @id
  title: String!
  vedioSourse: String!
  vedioUrl: String!
  subjects: [Subject!]! @realtion(name:"VedioToSubject",onDelete: SET_NULL)
  questions: [Questions!]! @realtion(name:"VedioToQuestions",onDelete: SET_NULL)
  order: Int!
  status: String!
}
type Questions {
  id: ID! @id
  title: String!
  videos: [Vedio!]! @realtion(name:"QuestionsToVedio",onDelete:CASCADE)
  correctAnswer: String
  bookTest: BookTest! @realtion(name:"QuestionsToBookTest",onDelete: SET_NULL)
  status: Int!
}





















